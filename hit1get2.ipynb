   "source": [
    "import math\n",
    "start = (1,1)\n",
    "end = (2,2)\n",
    "\n",
    "a = abs(end[0] - start[0])\n",
    "b = abs(end[1] - start[1])\n",
    "\n",
    "r = math.sqrt(a**2 + b**2) # sqrt 는 루트\n",
    "\n",
    "radian = math.atan(b/a)\n",
    "\n",
    "print(r, math.degrees(radian))"
   ]
  }
